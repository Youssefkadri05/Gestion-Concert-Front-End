oupe')\r\n            .then(response => response.json())\r\n            .then(data => {\r\n                // Quand la réponse est reçue, met à jour la propriété \"groupes\" avec les données JSON\r\n                this.groupes = data;\r\n            })\r\n    }\r\n}\r\n</script>"],"mappings":"AAcA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACH,OAAO;MACH;MACAC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN;IACAC,KAAK,CAAC,8BAA8B,EAC/BC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAChCF,IAAI,CAACJ,IAAG,IAAK;MACV;MACA,IAAI,CAACC,OAAM,GAAID,IAAI;IACvB,CAAC;EACT;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}