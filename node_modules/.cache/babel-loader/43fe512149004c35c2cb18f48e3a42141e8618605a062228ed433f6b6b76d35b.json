{"ast":null,"code":"import axios from 'axios';\nimport { Map, View } from 'ol';\nimport { Tile as TileLayer } from 'ol/layer';\nimport { OSM } from 'ol/source';\nexport default {\n  name: 'SallesDetails',\n  data() {\n    return {\n      salle: {\n        nom: 'Salle de réunion A',\n        adresse: '10 rue de la Paix, Paris'\n      }\n    };\n  },\n  methods: {\n    geocodeAddress() {\n      axios.get(`https://nominatim.openstreetmap.org/search?q=${encodeURIComponent(this.salle.adresse)}&format=json&limit=1`).then(response => {\n        const data = response.data[0];\n\n        // Mettre à jour la vue de la carte avec les coordonnées géographiques\n        const view = new View({\n          center: [parseFloat(data.lon), parseFloat(data.lat)],\n          zoom: 14\n        });\n        const osmLayer = new TileLayer({\n          source: new OSM()\n        });\n\n        // eslint-disable-next-line no-unused-vars\n        const map = new Map({\n          target: 'map',\n          layers: [osmLayer],\n          view: view\n        });\n      });\n    }\n  },\n  mounted() {\n    // Appeler la méthode pour géocoder l'adresse et afficher la carte\n    this.geocodeAddress();\n  }\n};","map":{"version":3,"names":["axios","Map","View","Tile","TileLayer","OSM","name","data","salle","nom","adresse","methods","geocodeAddress","get","encodeURIComponent","then","response","view","center","parseFloat","lon","lat","zoom","osmLayer","source","map","target","layers","mounted"],"sources":["C:\\Users\\33625\\Desktop\\M1\\System d'information\\projet\\projetVueJS\\concert\\src\\components\\SalleDetails.vue"],"sourcesContent":["<template>\r\n    <div>\r\n\r\n        <div id=\"map\"></div>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport { Map, View } from 'ol';\r\nimport { Tile as TileLayer } from 'ol/layer';\r\nimport { OSM } from 'ol/source';\r\n\r\nexport default {\r\n    name: 'SallesDetails',\r\n    data() {\r\n        return {\r\n            salle: {\r\n                nom: 'Salle de réunion A',\r\n                adresse: '10 rue de la Paix, Paris'\r\n            }\r\n        };\r\n    },\r\n    methods: {\r\n        geocodeAddress() {\r\n            axios.get(`https://nominatim.openstreetmap.org/search?q=${encodeURIComponent(this.salle.adresse)}&format=json&limit=1`)\r\n                .then(response => {\r\n                    const data = response.data[0];\r\n\r\n                    // Mettre à jour la vue de la carte avec les coordonnées géographiques\r\n                    const view = new View({\r\n                        center: [parseFloat(data.lon), parseFloat(data.lat)],\r\n                        zoom: 14,\r\n                    });\r\n\r\n                    const osmLayer = new TileLayer({\r\n                        source: new OSM()\r\n                    });\r\n\r\n                    // eslint-disable-next-line no-unused-vars\r\n                    const map = new Map({\r\n                        target: 'map',\r\n                        layers: [osmLayer],\r\n                        view: view,\r\n                    });\r\n                });\r\n        }\r\n    },\r\n    mounted() {\r\n        // Appeler la méthode pour géocoder l'adresse et afficher la carte\r\n        this.geocodeAddress();\r\n    }\r\n}\r\n</script>\r\n  "],"mappings":"AAQA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAG,EAAEC,IAAG,QAAS,IAAI;AAC9B,SAASC,IAAG,IAAKC,SAAQ,QAAS,UAAU;AAC5C,SAASC,GAAE,QAAS,WAAW;AAE/B,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE;QACHC,GAAG,EAAE,oBAAoB;QACzBC,OAAO,EAAE;MACb;IACJ,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAAA,EAAG;MACbZ,KAAK,CAACa,GAAG,CAAE,gDAA+CC,kBAAkB,CAAC,IAAI,CAACN,KAAK,CAACE,OAAO,CAAE,sBAAqB,EACjHK,IAAI,CAACC,QAAO,IAAK;QACd,MAAMT,IAAG,GAAIS,QAAQ,CAACT,IAAI,CAAC,CAAC,CAAC;;QAE7B;QACA,MAAMU,IAAG,GAAI,IAAIf,IAAI,CAAC;UAClBgB,MAAM,EAAE,CAACC,UAAU,CAACZ,IAAI,CAACa,GAAG,CAAC,EAAED,UAAU,CAACZ,IAAI,CAACc,GAAG,CAAC,CAAC;UACpDC,IAAI,EAAE;QACV,CAAC,CAAC;QAE