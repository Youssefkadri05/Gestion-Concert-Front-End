{"ast":null,"code":"import axios from 'axios';\nimport { Map, View } from 'ol';\nimport { Tile as TileLayer } from 'ol/layer';\nimport { OSM } from 'ol/source';\nexport default {\n  name: 'SallesDetails',\n  data() {\n    return {\n      salle: {\n        nom: 'Salle de réunion A',\n        adresse: '10 rue de la Paix, Paris'\n      }\n    };\n  },\n  methods: {\n    geocodeAddress() {\n      axios.get(`https://nominatim.openstreetmap.org/search?q=${encodeURIComponent(this.salle.adresse)}&format=json&limit=1`).then(response => {\n        const data = response.data[0];\n\n        // Mettre à jour la vue de la carte avec les coordonnées géographiques\n        const view = new View({\n          center: [parseFloat(data.lon), parseFloat(data.lat)],\n          zoom: 14\n        });\n        const osmLayer = new TileLayer({\n          source: new OSM()\n        });\n\n        // eslint-disable-next-line no-unused-vars\n        const map = new Map({\n          target: 'map',\n          layers: [osmLayer],\n          view: view\n        });\n      });\n    }\n  },\n  mounted() {\n    // Appeler la méthode pour géocoder l'adresse et afficher la carte\n    this.geocodeAddress();\n  }\n};","map":{"version":3,"names":["axios","Map","View","Tile","TileLayer","OSM","name","data","salle","nom","adresse","methods","geocodeAddress","get","encodeURIComponent","then","response","view","center","parseFloat","lon","lat","zoom","osmLayer","source","map","target","layers","mounted"],"sources":["C:\\Users\\33625\\Desktop\\M1\\System d'information\\projet\\projetVueJS\\concert\\src\\components\\SalleDetails.vue"],"sourcesContent":["<template>\r\n    <div>\r\n     \r\n      <div id=\"map\"></div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import { Map, View } from 'ol';\r\n  import { Tile as TileLayer } from 'ol/layer';\r\n  import { OSM } from 'ol/source';\r\n  \r\n  export default {\r\n    name: 'SallesDetails',\r\n    data() {\r\n      return {\r\n        salle: {\r\n          nom: 'Salle de réunion A',\r\n          adresse: '10 rue de la Paix, Paris'\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      geocodeAddress() {\r\n        axios.get(`https://nominatim.openstreetmap.org/search?q=${encodeURIComponent(this.salle.adresse)}&format=json&limit=1`)\r\n          .then(response => {\r\n            const data = response.data[0];\r\n  \r\n            // Mettre à jour la vue de la carte avec les coordonnées géographiques\r\n            const view = new View({\r\n              center: [parseFloat(data.lon), parseFloat(data.lat)],\r\n              zoom: 14,\r\n            });\r\n  \r\n            const osmLayer = new TileLayer({\r\n              source: new OSM()\r\n            });\r\n  \r\n            // eslint-disable-next-line no-unused-vars\r\nconst map = new Map({\r\n  target: 'map',\r\n  layers: [osmLayer],\r\n  view: view,\r\n});\r\n          });\r\n      }\r\n    },\r\n    mounted() {\r\n      // Appeler la méthode pour géocoder l'adresse et afficher la carte\r\n      this.geocodeAddress();\r\n    }\r\n  }\r\n  </script>\r\n  "],"mappings":"AAQE,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAG,EAAEC,IAAG,QAAS,IAAI;AAC9B,SAASC,IAAG,IAAKC,SAAQ,QAAS,UAAU;AAC5C,SAASC,GAAE,QAAS,WAAW;AAE/B,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;QACLC,GAAG,EAAE,oBAAoB;QACzBC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACfZ,KAAK,CAACa,GAAG,CAAE,gDAA+CC,kBAAkB,CAAC,IAAI,CAACN,KAAK,CAACE,OAAO,CAAE,sBAAqB,EACnHK,IAAI,CAACC,QAAO,IAAK;QAChB,MAAMT,IAAG,GAAIS,QAAQ,CAACT,IAAI,CAAC,CAAC,CAAC;;QAE7B;QACA,MAAMU,IAAG,GAAI,IAAIf,IAAI,CAAC;UACpBgB,MAAM,EAAE,CAACC,UAAU,CAACZ,IAAI,CAACa,GAAG,CAAC,EAAED,UAAU,CAACZ,IAAI,CAACc,GAAG,CAAC,CAAC;UACpDC,IAAI,EAAE;QACR,CAAC,CAAC;QAEF,MAAMC,QAAO,GAAI,IAAInB,SAAS,CAAC;UAC7BoB,MAAM,EAAE,IAAInB,GAAG;QACjB,CAAC,CAAC;;QAEF;QACZ,MAAMoB,GAAE,GAAI,IAAIxB,GAAG,CAAC;UAClByB,MAAM,EAAE,KAAK;UACbC,MAAM,EAAE,CAACJ,QAAQ,CAAC;UAClBN,IAAI,EAAEA;QACR,CAAC,C