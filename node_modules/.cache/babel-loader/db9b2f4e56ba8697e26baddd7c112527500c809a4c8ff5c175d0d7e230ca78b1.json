oading: false,\n      salle: {},\n      id_salle: this.$route.params.id\n    };\n  },\n  mounted() {\n   \n    this.getSalleData(this.id_salle);\n  },\n  methods: {\n    getSalleData(id_salle) {\n      fetch(`http://localhost:8080/salle/${id_salle}`)\n        .then(response => response.json())\n        .then(data => {\n          this.loading = true;\n          this.salle = data;\n          const address = this.salle.sal_adresse;\n          this.getMapData(address);\n        })\n        .catch(error => console.error(error));\n    },\n  },\n};\n</script>\n"],"mappings":"AAiCA,eAAe;EAEbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE,CAAC,CAAC;MACTC,QAAQ,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC;IAC/B,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IAER,IAAI,CAACC,YAAY,CAAC,IAAI,CAACL,QAAQ,CAAC;EAClC,CAAC;EACDM,OAAO,EAAE;IACPD,YAAYA,CAACL,QAAQ,EAAE;MACrBO,KAAK,CAAE,+BAA8BP,QAAS,EAAC,EAC5CQ,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAChCF,IAAI,CAACX,IAAG,IAAK;QACZ,IAAI,CAACC,OAAM,GAAI,IAAI;QACnB,IAAI,CAACC,KAAI,GAAIF,IAAI;QACjB,MAAMc,OAAM,GAAI,IAAI,CAACZ,KAAK,CAACa,WAAW;QACtC,IAAI,CAACC,UAAU,CAACF,OAAO,CAAC;MAC1B,CAAC,EACAG,KAAK,CAACC,KAAI,IAAKC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC,CAAC;IACzC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}